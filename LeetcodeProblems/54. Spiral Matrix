class Solution {
public:
    vector<int> spiralOrder(vector<vector<int>>& matrix) {
        int rows=matrix.size();
        int col=matrix[0].size();
        int x=0;int y=0;
        int dx=1;int dy=0;
        vector<int>res;
        for(int i=0;i<rows*col;i++){
            res.push_back(matrix[y][x]);
            matrix[y][x]=-101;
        
        int nx=x+dx;
        int ny=y+dy;
        if(nx<0||nx>=col||ny<0||ny>=rows||matrix[ny][nx]==-101){
            int temp=dx;
             dx=-dy;
             dy=temp;

        }
        x+=dx;
        y+=dy;}
        return res;
    }
};
